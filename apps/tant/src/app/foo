console.log("Uploading", item.localUri);

                const { signedUrl, fileKey } = await getPresignedUrl(
                  item.localUri
                );

                console.log("Got signed url", signedUrl, fileKey);

                const response = await fetch(signedUrl, {
                  method: "PUT",
                  body: item.localUri,
                }).catch((err) => {
                  console.error("Failed to upload", err);
                  return { ok: false, err: err };
                });

                console.log("Uploaded", item.remoteUri);

                if (response.ok) {
                  item.status = "uploaded";
                  item.remoteUri = fileKey;
                  setFileList((fileList) => [...fileList]);
                } else {
                  item.status = "failed";
                  setFileList((fileList) => [...fileList]);
                }
              }